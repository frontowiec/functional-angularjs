    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.8",
    "angular-mocks": "^1.6.3",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "css-loader": "^0.27.3",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^4.5.1",
